{
  "name": "stock-analyzer",
  "version": "1.0.0",
  "description": "股票分析软件 - 实时行情监控、趋势分析、投资建议",
  "main": "index.js",
  "scripts": {
    "dev": " concurrently \"npm run dev:web\" \"npm run dev:server\"",
    "build": "npm run build:web && npm run build:server",
    "dev:web": "cd packages/web && npm run dev",
    "dev:server": "cd services/server && npm run dev",
    "build:web": "cd packages/web && npm run build",
    "build:server": "cd services/server && npm run build",
    "test": "jest",
    "lint": "eslint . --ext .ts,.tsx",
    "prepare": "husky install"
  },
  "keywords": [
    "stock",
    "analysis",
    "finance",
    "trading",
    "investment"
  ],
  "author": "linhaiday",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "concurrently": "^8.0.0",
    "eslint": "^8.0.0",
    "eslint-config-react-app": "^7.0.0",
    "husky": "^8.0.0",
    "jest": "^29.0.0",
    "prettier": "^3.0.0",
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "redux": "^4.2.1",
    "antd": "^5.0.0",
    "echarts": "^5.4.0",
    "socket.io-client": "^4.7.0",
    "axios": "^1.6.0"
  }
}